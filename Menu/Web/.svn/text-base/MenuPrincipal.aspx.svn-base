<%@ Page Language="C#" AutoEventWireup="true" CodeFile="MenuPrincipal.aspx.cs" Inherits="Menu_Web_MenuPrincipal" %>

<!DOCTYPE html>

<html xmlns="http://www.w3.org/1999/xhtml">
<head runat="server">
    <title>MENU</title>
    <meta charset="utf-8" />
    <meta http-equiv="content-language" content="es" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,IE=EmulateIE11,IE=EmulateIE10,IE=EmulateIE9,IE=11,IE=10,IE=9,IE=8" />

    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=Edge,chrome=1;" />
    <link rel="stylesheet" type="text/css" href="../../Content/css/bootstrap.css" />
    <link href="../../General/Estilos/StyleSAT.css" rel="stylesheet" />
    <link rel="stylesheet" type="text/css" href="../../General/font-awesome/css/font-awesome.css" />
    <script type="text/javascript" src="../../ScriptsJQ/JQuery/Jquery-v3.5.1.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            //$("#trJMenu").menu({ position: { my: "left top", at: "left bottom" } });           
            fnVariablesSesion();
        });

        function fnVariablesSesion() {
            $.ajax({
                type: "POST",
                url: "MenuPrincipal.aspx/GetVariables",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (response) {
                    var result = response.d;
                    if (result.length > 0) {
                        var objRes = jQuery.parseJSON(result);
                        strPerfil = objRes.RFC;
                        //alert(strPerfil);
                        $("#hdnRFC").val(strPerfil);
                    }
                },
                failure: function (response) {
                    alert(response.d);
                }
            });
        }
    </script>
</head>
<body>
    <div>
        <div class="container-fluid">
            <div class="row m-auto">
                <!-- Menú Cobranza-->
                <div class="col">
                    <div class="card card-personal my-5 m-auto px-2 pt-4 border-2">
                        <span class="fa fa-money fa-3x logoMenu"></span>
                        <div class="card-body py-4 px-0">
                            <h5 class="card-title text-center pb-4 fw-bolder px-0">COBRANZA</h5>
                            <div class="text-center"><i class="fa fa-angle-double-down fa-lg" style="color: #691C32;"></i></div>
                            <div class="list-group mostrarSubmenu rounded-3 pt-1" style="height: 200px;">
                                <a href="#" class="list-group-item list-group-item-action ps-1 py-1 opcionSubmenu border-0 " onclick="top.fnEncriptaParametros('CartasInvitacion');"><span class="fa fa-chevron-circle-right" style="padding-right: 5px"></span>Cartas Invitación</a>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- Planeación-->
                <div class="col">
                    <div class="card card-personal my-5 m-auto px-2 pt-4 border-2">
                        <i class="fa fa-line-chart fa-3x logoMenu"></i>
                        <div class="card-body py-4 px-0">
                            <h5 class="card-title text-center pb-4 fw-bolder px-0">PLANEACIÓN</h5>
                            <div class="text-center"><i class="fa fa-angle-double-down fa-lg" style="color: #691C32;"></i></div>
                            <div class="list-group mostrarSubmenu rounded-3 pt-1" style="height: 200px;">

                                <a href="#" class="list-group-item list-group-item-action ps-1 py-1 opcionSubmenu border-0 " onclick="top.fnEncriptaParametros('Plantilla');"><span class="fa fa-chevron-circle-right" style="padding-right: 5px"></span>Plantilla AGR</a>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- Menú Admionistración de Usuarios-->
                <div class="col">
                    <div class="card card-personal my-5 m-auto px-2 pt-4 border-2">
                        <span class="fa fa-user fa-3x logoMenu"></span>
                        <div class="card-body py-4 px-0">
                            <h5 class="card-title text-center pb-4 fw-bolder px-0">ADMINISTRACIÓN DE USUARIOS</h5>
                            <div class="text-center"><i class="fa fa-angle-double-down fa-lg" style="color: #691C32;"></i></div>
                            <div class="list-group mostrarSubmenu rounded-3 pt-1" style="height: 200px;">
                                <a href="#" class="list-group-item list-group-item-action ps-1 py-1 opcionSubmenu border-0 " onclick="top.fnEncriptaParametros('SISGCR');"><span class="fa fa-chevron-circle-right" style="padding-right: 5px"></span>SISGCR</a>
                                <a href="#" class="list-group-item list-group-item-action ps-1 py-1 opcionSubmenu border-0 " onclick="top.fnEncriptaParametros('Responsivas');"><span class="fa fa-chevron-circle-right" style="padding-right: 5px"></span>Responsivas MAT</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <input type="hidden" id="hdnRFC" />
    <script src="../../Content/js/bootstrap.bundle.js"></script>
</body>
</html>
